<div class="container">
  <div class="row">
    <h4>免許新規登録</h4>
    <%= form_with model: @license, url: licenses_path, method: :post, local: false do |f| %>
      <%= f.text_field :name %>
      <%= f.submit "新規登録" %>
    <% end %>
  </div>
  <div class="row">
    <table class="table">
      <thead>
        <tr>
          <th scope="col"></th>
          <th scope="col"></th>
        </tr>
      </thead>
      <tbody class="license-btn">
        <%= render 'index', licenses: @licenses, possession_license: @possession_license %>
      </tbody>
    </table>
    <%= paginate @licenses %>
  </div>
</div>