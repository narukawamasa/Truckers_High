<table>
  <tbody>
    <% review.review_comments.each do |review_comment| %>
    <tr>
      <% if review_comment.deletion == true %>
        <td>管理者から違反コメントと診断されました</td>
      <% elsif review_comment.deletion == false %>
        <td><%= review_comment.comment %>　<%= review_comment.created_at.strftime("%Y-%m-%d %H:%M:%S") %></td>
      <% end %>
    <% end %>
    </tr>
  </tbody>
</table>